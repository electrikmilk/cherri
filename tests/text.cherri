/*
Easily edit text...
*/

@textVar = "test"

@textVar = correctSpelling(textVar)
@textVar = replaceText("s","a",textVar)
@textVar = replaceText("s","a",textVar, false)
@textVar = replaceText("[0-9a-zA-Z]","a",textVar, true, true)
@textVar = replaceText("[0-9a-z]","a",textVar, false, true)
@textVar = splitText(textVar,",")
@textVar = joinText(textVar,",")
@textVar = uppercase(textVar)
@textVar = lowercase(textVar)
@textVar = titleCase(textVar)
@textVar = capitalize(textVar)
@textVar = capitalizeAll(textVar)
@textVar = alternateCase(textVar)