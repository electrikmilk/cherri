function fibonacci(number n) {
    if @n <= 1 {
        output("{@n}")
    } else {
        const minusOne = @n - 1
        const minusTwo = @n - 2
        const fib1 = fibonacci(minusOne)
        const fib2 = fibonacci(minusTwo)
        const added = fib1 + fib2
        output("{added}")
    }
}

const output = fibonacci(7)
show("{output}")
